{
  "name": "isomorphic-piwik",
  "version": "1.0.0",
  "description": "An isomorphic wrapper around the HTTP Piwik API",
  "main": "index.js",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "precommit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier --parser babylon --write",
      "git add"
    ],
    "*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "*.graphql": [
      "prettier --parser graphql --write",
      "git add"
    ]
  },
  "repository": "https://github.com/blerpapp/isomorphic-piwik.git",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^5.0.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^8.0.19",
    "@types/query-string": "^5.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.3.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.2"
  }
}
